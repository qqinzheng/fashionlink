    <div class="section" id="AboutUs">
      <div class="section-inner">
          <h5 class="title">
            <%= i18n("About us")%>
            <div class="decoration"><i class="line-left"></i><i class="rhomebus_min"></i><i class="line-right"></i></div>
          </h5>
          <div class="aboutus">
              <div class="box clearfix">
                  <div class="left">
                    <div class="video fit">
                      <video id="my-player" class="video-js" height="100%" width="100%" controls poster="<%=cndUrl%>/images/video-poster.png" preload="auto" data-setup='{}'>
                        <source src="<%=videoCdnUrl%>/lianshang.mp4" type="video/mp4"></source>
                      </video>
                    </div>
                  </div>
                  <div class="right">
                    <div class="vertical-middle des-box fit">
                        <h5 class="des-title"><%=i18n('The introduction of Lianshang Group')%><i class="line"></i></h5>
                        <div class="des-content"><%=i18n('Introduction')%></div>
                    </div>
                  </div>
              </div>
              <div class="box clearfix">
