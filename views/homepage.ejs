          <a class="nav-item"  href="javascript:void(0)" data-target="#Advantages"><%= i18n("Advantages")%><i class="line"></i></a>
          <a class="nav-item" href="javascript:void(0)" data-target="#Services"><%= i18n("Services")%><i class="line"></i></a>
