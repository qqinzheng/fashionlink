        <div class="swiper-wrapper">
            <div class="swiper-slide">
              <img src="<%=cndUrl%>/images/banner_01_<%=locale%>.jpg"/>
            </div>
            <div class="swiper-slide banner-2">
              <img src="<%=cndUrl%>/images/banner_02_<%=locale%>.jpg"/>
