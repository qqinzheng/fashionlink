                    <div class="vertical-middle des-box fit">
                        <h5 class="des-title"><%=i18n('Vision of Lianshang')%><i class="line"></i></h5>
                        <div class="des-content"><%=i18n('Vision description')%></div>
                    </div>
                  </div>
                  <div class="right fit"><img class="fit-width" src="<%=cndUrl%>/images/vision.jpg"/></div>
              </div>
          </div>
      </div>
    </div>
